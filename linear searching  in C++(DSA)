#include<stdio.h>
#include<conio.h>
#include<math.h>
int main()
{
	int arr[20],key,n,i=0;
	printf("Enter the no. of element :");
	scanf("%d",&n);
	printf("Enter the element of array:\n");
	
	for(i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
		
	}
	printf("Enter the key you want to search:");
	scanf("%d",&key);
	i=0;
	while(i!=n-1)
	{
	
	if(key==arr[i])
	{
		printf("The key is in index [%d]",i);
		 goto exit;
	}
	else
	i++;
	
    }
	
	exit:
		
	return 0;
}
